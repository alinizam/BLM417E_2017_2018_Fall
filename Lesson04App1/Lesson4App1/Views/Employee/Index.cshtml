@{
    @model List<Lesson4App1.Models.Employee>
}

<div>
    <table>
        <tr> <th>Id</th><th>Name</th><th>Surname</th></tr>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.PersonelId</td>
                <td>@person.FirstName</td>
                <td>@person.LastName</td>
            </tr>
        }
    </table>

</div>

